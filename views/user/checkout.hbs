        function applyCoupon() {
            console.log('coupon trig 1')
            const couponCode = document.getElementById("couponCode").value;
            const totalAmount = document.getElementById("totalValue").textContent;

            fetch("/coupon-validate", {
                method: "POST",
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    couponCode: couponCode,
                    totalAmount: totalAmount
                })
            }).then(response => response.json())
              .then(response => {
                  if (response.status == false) {
                      document.getElementById("invalidCoupon").style.display = "block";
                      document.getElementById("validCoupon").style.display = "none";
                      document.getElementById("discountTotal").style.display = "none"
                      document.getElementById("onlyTotal").style.display = "block"
                      document.getElementById("removeBtn").style.display = "none" 
                  } else {
                      document.getElementById("invalidCoupon").style.display = "none";
                      document.getElementById("validCoupon").style.display = "block";
                      document.getElementById("onlyTotal").style.display = "none"
                      document.getElementById("removeBtn").style.display = "block"
                      document.getElementById("discountTotal").style.display = "block"
                      document.getElementById('discountPrice').innerText = response.discountTotal;
                  }
              })
        }

